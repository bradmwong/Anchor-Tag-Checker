<h1>HOME PAGE</h1>

<form id="form" action="/update" method="POST">
    <input type="text" id="siteMapUrl" name="siteMapUrl" required>
    <input type="submit" value="Submit">
</form>

<% if (siteMapUrl) { %>
    <h1>Links for SiteMap URL: <%= siteMapUrl %></h1>
<% }; %>

<% links.forEach(function(link) { %>
    <li><%= link %></li>
<% }); %>